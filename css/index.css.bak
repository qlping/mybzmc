  right:13px;
  top:60px;
  width:150px;
  height:160px;
  z-index:9999;
}
/* 轮播图 */
.banner{
  width:100%;
  height:650px;
  overflow:hidden;
}
.banner .fullSlide{
  width:100%;
  position:relative;
  height:650px;
}
.banner .fullSlide ul{
  width:100% !important;
  height:650px;
  position:relative;
}
.banner .fullSlide ul li{
  width:100% !important;
  height:650px;
  overflow:hidden;
  text-align:center;
  background-color:#f0f0f0;
  background-image:url(../img/index/banner.png);
  background-position:50% 0px;
  background-repeat:no-repeat;
  position:absolute;
  left:0px;
  top:0px;
}
.banner .fullSlide ul li a{
  display:block;
  height:650px;

}
.fullSlide .prev{
   left:20px;
   background-position:-33px 0;
   position:absolute;
   display:block;
   width:33px;
   height:43px;
   top:303px;/**/
   background:url(../img/index/slider_arrow.png) 0 0 no-repeat;
   opacity:1;
} 
.fullSlide .next{
  right:20px;
  background-position:0 0; 
  position:absolute;
  display:block;
  width:33px;
  height:43px;
  top:303px;
  background:url(../img/index/slider_arrow.png) 0 0 no-repeat;
  opacity:1;
 } 
