/* 头部 */
.header{
  height:103px;
  background-color:#B6D3B5;
}
.logo{
  margin-left:40px !important;
}
.nav{
  margin-left:40px;
}
.nav{
  display:block;
  -webkit-magin-before:1em;
  -webkit-magin-after:1em;
  -webkit-magin-start:0px;
  -webkit-magin-end:0px;
  -webkit-padding-estart:40px;
}
.nav li{
  float:left;
  display:inline-block;
  height:100px;
  text-align:center;
}
.nav li a{
  position:relative;
  display:block;
  height:100px;
  margin:0 20px;
  font-size:18px;
  color:#000;
  padding-top:30px;
  font-family:"Microsoft Yahei";
  line-height:20px;
}
.nav li p{
  font-size:12px;
  display:block;
  -webkit-magin-before:1em;
  -webkit-magin-after:1em;
  -webkit-magin-start:0px;
  -webkit-magin-end:0px;
}
.nav li a .line{
  position:absolute;
  left:0;
  bottom:20px;
  width:0;
  height:2px;
  background-color:#fff;
  transition:width .5s;
  -moz-transition:width .5s;
  -webkit-transition:winth .5s;
  -o-transition:width .5s;
}
.list{
  margin-right:50px !important;
  margin-top:-66px !important;
}
.list a{
  color:#000;
}
.list .login a{
  color:#000; 
}
.list .login span{
  margin-left:5px !important;
  margin-right:5px !important;
}
.list .shopping-car{
  background:url(../img/index/icon_shoppingcar.gif) no-repeat left center;
  padding-left:25px;
  padding-right:10px;
  position:relative;
  margin-left:20px !important;
  margin-top:-20px;
}
.list .shopping-car a{
  display:block;
  margin-top:20px;
  color:#000;
}
.list .shopping-car span{
  display:inline-block;
  position:relative;
  right:0;
  top:-8px;
  height:16px;
  line-height:15px;
  z-index:300;
}
.list .lxkf{
